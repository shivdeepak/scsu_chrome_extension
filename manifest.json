{
  "manifest_version": 3,
  "name": "YC Startup School - Fix LinkedIn URL",
  "version": "1.0.0",
  "description": "This extension fixes broken 'View on LinkedIn' button on the YC Startup School website.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://startupschool.org/*", "*://*.startupschool.org/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}